import java.util.*;
class demo{
public static void main(String args[]){
Scanner sc = new Scanner(System.in);
System.out.println("enter the size of the array:");
int n = sc.nextInt();
int a[] = new int[n];
int largest = a[0];
System.out.println("enter array elements:");
for(int i=0;i<n;i++)
{
a[i] = sc.nextInt();
}
for(int i=0;i<n;i++){
if(a[i]>largest)
{
largest=a[i];
}
}
System.out.println("the largest number in the series is:"+largest);
}
}

